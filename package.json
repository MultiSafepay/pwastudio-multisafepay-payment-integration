{
  "name": "@multisafepay/multisafepay-payment-integration",
  "author": "integration@multisafepay.com",
  "version": "1.0.2.1",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.qlicks.nl"
  },
  "main": "src/index.js",
  "pwa-studio": {
    "targets": {
      "intercept": "src/intercept.js"
    }
  },
  "scripts": {
    "format": "prettier --ignore-path .gitignore \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\"src/**/*.+(ts|js|tsx)\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\" --write",
    "lint": "eslint --ignore-path .gitignore 'src/**/{*.js,package.json}'",
    "test": "jest",
    "test:dev": "jest --watch",
    "prepare": "install-peers"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "peerDependencies": {
    "@magento/peregrine": "~11.0.0",
    "@magento/pwa-buildpack": "~10.0.0",
    "@magento/venia-ui": "~8.0.0",
    "react": "~17.0.1",
    "react-intl": "~5.7.0",
    "react-router-dom": "~5.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@magento/peregrine": "~11.0.0",
    "@magento/pwa-buildpack": "~10.0.0",
    "@magento/venia-ui": "~8.0.0"
  }
}
